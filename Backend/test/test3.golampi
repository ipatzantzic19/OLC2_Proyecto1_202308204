// Ejemplo 3: Operaciones aritméticas y comparaciones
func main() {
    var a int32 = 15
    var b int32 = 7
    var c float32 = 3.5
    
    fmt.Println("Suma:", a + b)
    fmt.Println("Resta:", a - b)
    fmt.Println("Multiplicación:", a * b)
    fmt.Println("División:", a / b)
    fmt.Println("Módulo:", a % b)
    
    fmt.Println("Mayor que:", a > b)
    fmt.Println("Menor que:", a < b)
    fmt.Println("Igual:", a == b)
    
    fmt.Println("Suma mixta:", a + c)
}